<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.per2.apexlegendsguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
        </activity>
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".AmmoActivity" />
        <activity android:name=".LegendActivity" />
        <activity android:name=".ARActivity" />
        <activity android:name=".LMGActivity" />
        <activity android:name=".PistolActivity" />
        <activity android:name=".ShotgunActivity" />
        <activity android:name=".SMGActivity" />
        <activity android:name=".SniperActivity" />
        <activity android:name=".IndividualLegendActivity" />
        <activity android:name=".IndividualGunActivity" />
        <activity android:name=".MainGunActivity"></activity>
    </application>

</manifest>